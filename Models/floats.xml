<?xml version="1.0" encoding="ISO-8859-1"?>

<PropertyList>

  <path>floats.ac</path>

  <animation>
    <!-- Objets opaques -->
    <object-name>flotteurs</object-name>
    <object-name>axes</object-name>
    <object-name>gouvernailG</object-name>
    <object-name>gouvernailD</object-name>
    <object-name>caches</object-name>

    <!-- Objets transparents -->
  </animation>

  <animation>
    <type>material</type>
    <object-name>flotteurs</object-name>
    <object-name>axes</object-name>
    <object-name>gouvernailG</object-name>
    <object-name>gouvernailD</object-name>
    <object-name>caches</object-name>

    <property-base>sim/model/livery</property-base>
    <texture-prop>texture</texture-prop>
    <texture>texture.png</texture>
  </animation>

  <!-- Cranking effects -->
  <!-- Left -->
  <model>
    <path>Aircraft/LATE-29/Models/Effects/cranking/cranking.xml</path>
    <offsets>
      <x-m> -5.365 </x-m>
      <y-m> -0.462 </y-m>
      <z-m>  0.984 </z-m>
    </offsets>
  </model>

  <!-- Right -->
  <model>
    <path>Aircraft/LATE-29/Models/Effects/cranking/cranking.xml</path>
    <offsets>
      <x-m> -5.365 </x-m>
      <y-m>  0.462 </y-m>
      <z-m>  0.984 </z-m>
    </offsets>
  </model>

  <!-- Trainée gauche -->
  <model>
    <path>Aircraft/LATE-29/Models/Effects/waves/wakeG.xml</path>
    <offsets>
      <x-m>  0.992 </x-m>
      <y-m> -1.640 </y-m>
      <z-m> -1.594 </z-m>
      <pitch-deg> -45 </pitch-deg>
    </offsets>
  </model> 

  <!-- Trainée droite -->
  <model>
    <path>Aircraft/LATE-29/Models/Effects/waves/wakeD.xml</path>
    <offsets>
      <x-m>  0.992 </x-m>
      <y-m>  1.640 </y-m>
      <z-m> -1.594 </z-m>
      <pitch-deg> -45 </pitch-deg>
    </offsets>
  </model> 

  <!-- Eclaboussures, avant gauche -->
  <model>
    <path>Aircraft/LATE-29/Models/Effects/waves/jetG.xml</path>
    <offsets>
      <x-m> -6.223 </x-m>
      <y-m> -1.640 </y-m>
      <z-m> -1.852 </z-m>
      <pitch-deg> -90 </pitch-deg>
    </offsets>
  </model> 

  <!-- Eclaboussures, avant droite -->
  <model>
    <path>Aircraft/LATE-29/Models/Effects/waves/jetD.xml</path>
    <offsets>
      <x-m> -6.223 </x-m>
      <y-m>  1.640 </y-m>
      <z-m> -1.852 </z-m>
      <pitch-deg> -90 </pitch-deg>
    </offsets>
  </model> 

  <!-- Eclaboussures, centre gauche -->
  <model>
    <path>Aircraft/LATE-29/Models/Effects/waves/jetmG.xml</path>
    <offsets>
      <x-m> -2.635 </x-m>
      <y-m> -1.640 </y-m>
      <z-m> -1.896 </z-m>
      <pitch-deg> -90 </pitch-deg>
    </offsets>
  </model> 

  <!-- Eclaboussures, centre droite -->
  <model>
    <path>Aircraft/LATE-29/Models/Effects/waves/jetmD.xml</path>
    <offsets>
      <x-m> -2.635 </x-m>
      <y-m>  1.640 </y-m>
      <z-m> -1.896 </z-m>
      <pitch-deg> -90 </pitch-deg>
    </offsets>
  </model> 

</PropertyList>

